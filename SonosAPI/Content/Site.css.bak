body{
	background-color: black;
}

#CurrentMeta {
    background-color: white;
    border: 2px solid black;
    border-radius: 25px;
    display: none;
    height: 150px;
    left: -100px;
	padding-left: 10px;
    padding-top: 5px;
    position: relative;
    top: -35px;
    width: 400px;
	box-shadow: 5px 5px 7px #888;
}	

#AudioIn {
    position: absolute;
    right: 20px;
    top: 390px;
	Background: URL("/Images/audioin.png");
	background-size:100% 100%;
	background-repeat:no-repeat;
	background-color:white;
}
#AudioIn.akt{
	box-shadow: 0 0 0 #888;
	Background-Color:grey;
}

#CurrentMeta:after {
    border-color: #000 transparent transparent;
    border-style: solid;
    border-width: 20px;
    bottom: -26%;
    content: "";
    height: 0;
    position: absolute;
    right: 60%;
    width: 0;
}
#Overlay{
	position:absolute;
	top:0;
	left:0;
	width:100%;
	height:100%;
	background:grey;
	display:none;
	border-radius: 25px 25px 25px 25px;
	border: 1px solid;	
	opacity:0.5;
	z-index:99;
}
#SleepModeButton{
	position:absolute;
	left:5px;
	bottom: 100px;
	Background: URL("/Images/sleepmode.png");
	background-size:100% 100%;
	background-repeat:no-repeat;
}
#SleepMode{
    background-color: white;
    border: 2px solid;
    border-radius: 25px;
    box-shadow: 5px 5px 7px #888;
    display: none;
    height: 100px;
    left: 90px;
    padding-left: 10px;
    padding-top: 10px;
    position: absolute;
    bottom: 80px;
    width: 110px;
}

#SleepModeButton.aktiv{
	background-color: grey;
}

#SleepModeWrapper{
	height: 100px;
	position:relative;
}

#SleepModeWrapper > div {
    text-align: center;
}
#SleepModeWrapper:after{
    border-color: transparent #000 transparent transparent;
    border-style: solid;
    border-width: 25px;
    content: "";
    height: 0;
    right: 110%;
    position: absolute;
    top: 25%;
    width: 0;
}
#SleepModeSave{
    border: 2px solid;
